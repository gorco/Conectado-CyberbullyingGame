{
	"bed":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I'll sleep later once I come back from school."
			}]
		}		
	},
	"init":{
		"root": {
			"type": "fork",
			"options": [{
				"next": "node0",
				"condition": "varObject('global', 'RepeatedNonStatic') == true"
			}]
		},
		"node0": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "Is it me?... or the phone was showing yesterdays' date????"
			},
			{
				"tag": "You",
				"msn": "Is it broken or something?"
			}]
		}
	},
	"wardrobeInside1":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I don't need anything from this right now."
			}]
		}		
	},
	"wardrobeInside2":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I have to go to class now and haven't organized this yet. I'll need to find another time."
			}]
		}		
	},
	"bag":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "Everything's ready, let's go."
			}]
		},
		"node1": {
			"type": "event",
			"event": {
				"name": "pick",
				"var": "BagPicked",
				"value": true
			}
		},
	},
	"computer":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "I should wait until the afternoon or IÂ´ll be late."
			},
			{
				"tag": "PC",
				"msn": "Do you want to turn on the computer?"
			}]
		},		
		"node1": {
			"type": "options",
			"questionId": "plugInMobile",
			"options": [{
				"next": "node1a",
				"msn": "Yes"
			},
			{
				"next": "node1b",
				"msn": "No"
			}]
		},
		"node1a": {
			"type": "event",
			"next": "moveCamera",
			"event":{
				"name": "computerON"
			}
		}, 
		"moveCamera": {
			"type": "event",
			"next": "changeHour",
			"event":{
				"name": "move camera",
				"key": "computer"
			}
		},
		"changeHour": {
			"type": "event",
			"event":{
				"name": "change variable",
				"var": "hour",
				"value": "8:10",
			}
		},
		"node1b": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I'd better leave."
			}]
		}		
	},
	"plug":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "Do I leave the mobile charging?"
			}]
		},		
		"node1": {
			"type": "options",

			"options": [{
				"next": "node1a",
				"msn": "Yes"
			},
			{
				"next": "node1b",
				"msn": "No"
			}]
		},
		"node1a": {
			"type": "event",
			"event": {
				"name": "change state",
				"var": "ChargingMobile",
				"value": true,
				"state": 0
			}
		}, 
		"node1b": {
			"type": "event",
			"event": {
				"name": "change state",
				"var": "ChargingMobile",
				"value": false,
				"state": 1
			}
		}		
	},
	"livingroomDoor" : {
		"root": {
			"type": "event",
			"event": {
				"name": "move camera",
				"key": "livingroom"
			}
		}
	}
}
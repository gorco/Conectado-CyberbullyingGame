{
	"bed":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "Now that i am already up i should not lay down again."
			}]
		}		
	},
	"wardrobeInside1":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "There is nothing useful."
			}]
		}		
	},
	"wardrobeInside2":{
		"root": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I have to organize all this eventually, but now i have to go to school."
			}]
		}		
	},
	"bag":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "I think i got everything prepared..."
			}]
		},
		"node1": {
			"type": "dialog",
			"next": "node2",
			"fragments": [{
				"tag": "You",
				"msn": "Aye, i got everything, better take the backpack and leave now."
			}]
		},
		"node2": {
			"type": "event",
			"event": {
				"name": "pick",
				"var": "BagPicked",
				"value": true
			}
		},
	},
	"computer":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "If i turn it on now, i will be late for sure."
			},
			{
				"tag": "You",
				"msn": "Better to wait for the afternoon."
			},
			{
				"tag": "PC",
				"msn": "Do you want to turn on the computer?"
			}]
		},		
		"node1": {
			"type": "options",
			"questionId": "turnOnPC",
			"options": [{
				"next": "node1a",
				"msn": "Yes"
			},
			{
				"next": "node1b",
				"msn": "No"
			}]
		},
		"node1a": {
			"type": "event",
			"next": "moveCamera",
			"event":{
				"name": "computerON"
			}
		}, 
		"moveCamera": {
			"type": "event",
			"next": "changeHour",
			"event":{
				"name": "move camera",
				"key": "computer"
			}
		},
		"changeHour": {
			"type": "event",
			"event":{
				"name": "change variable",
				"var": "hour",
				"value": "8:10",
			}
		},
		"node1b": {
			"type": "dialog",
			"fragments": [{
				"tag": "You",
				"msn": "I'd better leave now."
			}]
		}		
	},
	"plug":{
		"root": {
			"type": "dialog",
			"next": "node1",
			"fragments": [{
				"tag": "You",
				"msn": "Do i leave the mobile charging?"
			}]
		},		
		"node1": {
			"type": "options",
			"questionId": "plugInMobile",
			"options": [{
				"next": "node1a",
				"msn": "Si"
			},
			{
				"next": "node1b",
				"msn": "No"
			}]
		},
		"node1a": {
			"type": "event",
			"event": {
				"name": "change state",
				"var": "ChargingMobile",
				"value": true,
				"state": 0
			}
		}, 
		"node1b": {
			"type": "event",
			"event": {
				"name": "change state",
				"var": "ChargingMobile",
				"value": false,
				"state": 1
			}
		}		
	},
	"livingroomDoor" : {
		"root": {
			"type": "event",
			"event": {
				"name": "move camera",
				"key": "livingroom"
			}
		}
	}

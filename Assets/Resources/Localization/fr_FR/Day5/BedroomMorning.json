			"type": "dialog",
			"next": "node1",
			"fragments": [
				{
					"tag": "Toi",
					"msn": "Je vais tout prendre et partir."
				}
			]
		},
		"node1": {
			"type": "event",
			"event": {
				"name": "pick",

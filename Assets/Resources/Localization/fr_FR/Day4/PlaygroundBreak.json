			"type": "dialog",
			"next": "setter",
			"fragments": [
				{
					"tag": "Álex",
					"character": "Alex",
					"msn": "Et si c'était nous, que vas-tu faire ? Vas-tu pleurer et le dire à notre professeur, à tes parents...?"
				},
				{
					"tag": "Ana",
					"character": "Ana",
					"msn": "Hahaha, je suis sûre que oui."
				},
				{
					"tag": "Ana",
					"character": "Ana",
					"msn": "Allez, pars et va pleurer ailleurs."
				}
			]
		},
		"node1b": {
			"type": "dialog",
			"next": "jokeAccepted",
			"fragments": [{
				"tag": "Ana",
				"character": "Ana",
				"msn": "Peut-être"
			},
				{
					"tag": "Ana",
					"character": "Ana",
					"msn": "Nous te le ferons savoir si tu vas coller cette affiche dans le dos de María."
				},
			{
				"tag": "Ana",
				"character": "Ana",
				"msn": "(Elle te donne un papier avec du ruban adhésif et la phrase : \"Je suis masochiste, insultez-moi si vous lisez ceci\".)."
			},
				{
					"tag": "Ana",
					"character": "Ana",
					"msn": "Reviens quand tu l'auras fait... sinon, ne te donne pas la peine de revenir nous voir."
				}]
		},
		"jokeAccepted": {
			"type": "event",
			"next": "mariaQuest",
			"event": {
				"name": "change variable",
				"var": "MariaJoke",
				"value": 1,
			}
		},
		"mariaQuest": {
			"type": "event",
			"next": "setter",
			"event": {
				"name": "change variable",
				"var": "MariaQuest",
				"value": 1,
			}
		},
		"setter":{
			"type": "setter",
			"next": "repeat",
			"set": {
				"var": "spoken",
				"value": true
			}
		}
	},
	"schoolEnter": {
		"root": {
			"type": "event",
			"next": "time",
			"event": {
				"name": "move camera",
				"key": "stairs"
			}
		},
		"time": {
			"type": "event",
			"event": {
				"name": "change variable",
				"var": "TimeEnough",
				"value": true,
			}
		}
	},
	"schoolExit": {
		"root": {
			"type": "dialog",
			"fragments": [
				{
					"tag": "Toi",
					"msn": "C'est la pause, il y a encore quelques cours avant que je puisse rentrer chez moi..."
				}
			]
		}
	}
}
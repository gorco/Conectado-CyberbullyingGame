			"type": "dialog",
			"next": "deactive",
			"fragments": [{
				"tag": "You",
				"msn": "It's a picture of my face in a dog's body sticking out the tongue."
			},
			{
				"tag": "You",
				"msn": "Why? Who put this?"
			}]
		},

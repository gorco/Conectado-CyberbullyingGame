			},
			{
				"tag": "You",
				"msn": "With a very original sentence... \"A baby arrived to class\"."
			}]
		},
		"nogum": {
			"type": "event",
			"next": "nogumDialog",
			"event": {
				"name": "active object",
				"time": 0,
				"other": "photoDog"

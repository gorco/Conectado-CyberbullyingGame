				"character": "Alison",
				"msn": "Maybe you should talk with the teacher or the pricipal."
			}]
		},
		"node1B": {
			"type": "dialog",
			"next": "setter",
			"fragments": [{
				"tag": "Alison",
				"character": "Alison",
				"msn": "Someone has posted a picture of yours in every school board."
			},
			{
				"tag": "Alison",
				"character": "Alison",
				"msn": "It was also in the playground."
			},
			{
				"tag": "Alison",
				"character": "Alison",
				"msn": "I think you should go talk to any teacher available."
			}]
		},
		"setter":{
			"type": "setter",
			"set": {
				"var": "spoken",
				"value": true
			}
		},
	},
	"board":{
		"root": {
			"type": "event",
			"next": "forkNode",
			"event": {
				"name": "change variable",
				"var": "BoardQuest",
				"value": 1,
			}
		},
		"forkNode": {
			"type": "fork",
			"options": [{
				"next": "gum",
				"condition": "varObject('global', 'GumQuestNonStatic') == 0"
			},
			{
				"next": "washed",
				"condition": "varObject('global', 'GumQuestNonStatic') == 1"
			},
			{
				"next": "nogum",
				"condition": "varObject('global', 'GumQuestNonStatic') == 2"
			}]
		},
		"gum": {
			"type": "event",
			"next": "gumDialog",
			"event": {
				"name": "active object",
				"time": 0,
				"other": "photoGum"
			}
		},
		"gumDialog": {
			"type": "dialog",
